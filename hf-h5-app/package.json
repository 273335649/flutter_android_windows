{
  "name": "hm-mes-product-app",
  "version": "1.0.6",
  "private": true,
  "author": "ydd",
  "scripts": {
    "build:zs-hf-dev": "cross-env APP_ENV=dev max build",
    "build:zs-hf-prod": "cross-env APP_ENV=prod max build",
    "build:zs-hf-fat": "cross-env APP_ENV=test max build",
    "dev": "npm run dev:local",
    "dev:local": "cross-env APP_ENV=dev APP_LOCAL=true max dev",
    "dev:dev": "cross-env APP_ENV=dev max dev",
    "dev:prod": "cross-env APP_ENV=prod max dev",
    "dev:test": "cross-env APP_ENV=test max dev",
    "format": "prettier --write \"src/**/*.{js,jsx,tsx,ts,css,less,html,md,json}\"",
    "postinstall": "max setup",
    "setup": "max setup",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "lint:style": "stylelint \"src/**/*.{htm,html,css,less}\"",
    "lint:style:fix": "stylelint \"src/**/*.{htm,html,css,less}\" --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "prepare": "husky install",
    "start": "npm run dev"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@umijs/max": "^4.0.13",
    "antd": "^5.26.7",
    "axios": "^0.27.2",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react-vant": "^3.3.5"
  },
  "devDependencies": {
    "@ahooksjs/use-request": "^2.8.15",
    "@commitlint/cli": "^17.1.1",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "mddir": "^1.1.1",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^2",
    "prettier-plugin-packagejson": "^2",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.1.2"
  }
}
